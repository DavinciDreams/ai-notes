{
  "name": "ai-notes",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\" \"npm run dev:collaboration\"",
    "dev:frontend": "vite",
    "dev:backend": "nodemon --exec \"tsx\" server/index.ts",
    "dev:collaboration": "nodemon --exec \"tsx\" server/collaboration-minimal.ts",
    "build": "tsc -b && vite build",
    "build:backend": "tsc --project tsconfig.server.json",
    "start": "concurrently \"npm run start:backend\" \"npm run start:collaboration\"",
    "start:backend": "node dist/server/index.js",
    "start:collaboration": "node dist/server/collaboration.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^9.0.2",
    "@headlessui/react": "^2.2.4",
    "@heroicons/react": "^2.2.0",
    "@hocuspocus/provider": "^3.1.1",
    "@hocuspocus/server": "^3.1.1",
    "@tanstack/react-query": "^5.77.2",
    "@tiptap/extension-collaboration": "^2.12.0",
    "@tiptap/extension-collaboration-cursor": "^2.12.0",
    "@tiptap/pm": "^2.12.0",
    "@tiptap/react": "^2.12.0",
    "@tiptap/starter-kit": "^2.12.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/d3": "^7.4.3",
    "@types/jsonwebtoken": "^9.0.9",
    "axios": "^1.9.0",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "d3": "^7.9.0",
    "dotenv": "^16.5.0",
    "easy-speech": "^2.4.0",
    "elasticsearch": "^16.7.3",
    "express": "^5.1.0",
    "express-validator": "^7.2.1",
    "fabric": "^6.6.6",
    "framer-motion": "^12.14.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.511.0",
    "minio": "^8.0.5",
    "morgan": "^1.10.0",
    "multer": "^2.0.0",
    "neo4j-driver": "^5.28.1",
    "pg": "^8.16.0",
    "postgresql": "^0.0.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hot-toast": "^2.5.2",
    "react-router-dom": "^7.6.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^3.3.0",
    "ws": "^8.18.2",
    "y-protocols": "^1.0.6",
    "y-websocket": "^3.0.0",
    "yjs": "^13.6.27"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.15.21",
    "@types/pg": "^8.15.2",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^4.4.1",
